@model IEnumerable<KeyValuePair<int, Employee>>

@{
    //ViewData["Title"] = "Сотрудники";
    ViewBag.Title = "Сотрудники";
}

<div class="container">
    <h3>@ViewData["Title"]</h3>

    <hr />

    <table class="table table-bordered" id="employees-table">
        <thead>
            <tr>
                <th>id</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.Value.Id</td>
                    <td>@employee.Value.SurName</td>
                    <td>@employee.Value.FirstName</td>
                    <td>@employee.Value.Patronymic</td>
                    <td>@employee.Value.Age</td>
                    <td>
                        <a asp-controller="Employees" asp-action="Details" asp-route-Id="@employee.Value.Id">Карточка</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

@section Styles{
    <style>
        #employees-table {
            background: red
        }
    </style>
}

